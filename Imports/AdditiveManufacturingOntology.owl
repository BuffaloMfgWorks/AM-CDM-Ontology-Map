     -->

    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/AMSubsystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSubsystem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ComponentRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
        <cco:definition>A System that is bearer of some Component Role within an Additive Manufacturing System.</cco:definition>
        <rdfs:comment>This class represents subsystems within an Additive Manufacturing System, such as a laser system in a selective laser sintering machine or an extruder in a fused deposition modeling printer, which are essential for the system&apos;s operation and success in manufacturing processes.</rdfs:comment>
        <rdfs:label xml:lang="en">Additive Manufacturing Subsystem</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/agent_in"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
        <cco:definition>A System that is an agent in an Act Of Additive Manufacturing and is the bearer of some AM System Role.</cco:definition>
        <rdfs:comment>This class represents systems specifically designed to carry out additive manufacturing processes, often involving the deposition of materials layer by layer to form objects.</rdfs:comment>
        <rdfs:label xml:lang="en">Additive Manufacturing System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfManufacturing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/has_input"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/PortionofFeedstock"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfManufacturing"/>
        <cco:definition>An Act Of Manufacturing that involves the deposition of successive layers of material to create an object, using a PortionofFeedstock as input.</cco:definition>
        <cco:example_of_usage>Examples include 3D printing processes such as Fused Deposition Modeling (FDM), Selective Laser Sintering (SLS), and Stereolithography (SLA).</cco:example_of_usage>
        <rdfs:comment>This process typically involves the use of digital models to direct machinery in the layer-by-layer addition of material, resulting in a three-dimensional object. It is distinct from subtractive manufacturing, which removes material to form an object.</rdfs:comment>
        <rdfs:label xml:lang="en">Act of Additive Manufacturing</rdfs:label>
        <rdfs:label xml:lang="en">Layer-by-Layer Deposition Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/AdditiveManufacturingBuildSite -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/AdditiveManufacturingBuildSite">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000029"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000176"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildChamber"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000029"/>
        <cco:definition xml:lang="en">a site within a build chamber where the additive manufacturing process occurs.</cco:definition>
        <rdfs:label xml:lang="en">Additive Manufacturing Build Site</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/AdditiveManufacturingFacility -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/AdditiveManufacturingFacility">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ManufacturingFacility"/>
        <cco:definition xml:lang="en">a manufacturing facility that is designed for the production of goods using some additive manufacturing act</cco:definition>
        <rdfs:comment xml:lang="en">Note that it is often the case that subtractive manufacturing often occurs at additive manufacturing facilities.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/BinderJettingAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/BinderJettingAct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing"/>
        <cco:definition>An act of additive manufacturing where a liquid binding agent is selectively deposited onto a powder bed, binding the material layer by layer to form an object.</cco:definition>
        <rdfs:label xml:lang="en">Binder Jetting Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/BinderJettingSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/BinderJettingSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem"/>
        <cco:definition>An Additive Manufacturing System designed to deposit some portion of liquid binding feedstock in layers onto a powder bed of materials to create parts.</cco:definition>
        <rdfs:comment>This system is versatile and cost-effective, used for full-color prototypes and sand-casting molds.</rdfs:comment>
        <rdfs:label xml:lang="en">Binder Jetting System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/BuildChamber -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildChamber">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
        <cco:definition_source xml:lang="en">A build chamber is a material artifact that is designed to contain a site where some controlled manufacturing process occures</cco:definition_source>
        <rdfs:label xml:lang="en">Build Chamber</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/BuildInterruption -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildInterruption">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_part_of_process"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfManufacturing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <cco:definition xml:lang="en">A process that is part of an Act of Manufacturing which temporarily halts the manufacturing process.</cco:definition>
        <rdfs:comment xml:lang="en">This class represents any interruption that occurs during the manufacturing process, causing a temporary halt in production.</rdfs:comment>
        <rdfs:label xml:lang="en">Build Interruption</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/BuildPlatformRole -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildPlatformRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <cco:definition>A role that, if realized, is realized when a material artifact provides support during some act of additive manufacturing.</cco:definition>
        <rdfs:comment>This class represents the functional role of an object as a build platform during additive manufacturing processes.</rdfs:comment>
        <rdfs:label xml:lang="en">AM Build Platform Role</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/CeramicPlate -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/CeramicPlate">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/hasRole"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildPlatformRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <cco:definition>A ceramic surface used as a build platform in additive manufacturing processes, supporting the build material and the part being fabricated.</cco:definition>
        <rdfs:comment>This class represents ceramic plates commonly used as build platforms in various additive manufacturing methods.</rdfs:comment>
        <rdfs:label xml:lang="en">Ceramic Plate</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/DirectedEnergyDepositionAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/DirectedEnergyDepositionAct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing"/>
        <cco:definition>An act of additive manufacturing where focused thermal energy is used to melt and deposit material to form layers.</cco:definition>
        <rdfs:label xml:lang="en">Directed Energy Deposition Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/DirectedEnergyDepositionSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/DirectedEnergyDepositionSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem"/>
        <cco:definition>An Additive Manufacturing System designed to use a laser or electron beam to melt some portion of feedstock and deposit it around a fixed object, forming layers as it hardens.</cco:definition>
        <rdfs:comment>This system is used to create and repair parts, utilizing materials such as metal powder, ceramics, wires, or polymers.</rdfs:comment>
        <rdfs:label xml:lang="en">Directed Energy Deposition System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/FeedstockRecyclingAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/FeedstockRecyclingAct">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfArtifactProcessing"/>
        <cco:definition>An act of artifact manipulation where previously used feedstock is reprocessed for reuse in future manufacturing cycles.</cco:definition>
        <rdfs:comment>This class covers processes that involve the collection, cleaning, and reprocessing of used feedstock materials, such as metal powders or polymers, to make them suitable for reuse in subsequent manufacturing operations.</rdfs:comment>
        <rdfs:label xml:lang="en">Feedstock Recycling Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/GlassPlate -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/GlassPlate">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/hasRole"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildPlatformRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <cco:definition>A glass surface used as a build platform in additive manufacturing processes, supporting the build material and the part being fabricated.</cco:definition>
        <rdfs:comment>This class represents glass plates commonly used as build platforms in various additive manufacturing methods.</rdfs:comment>
        <rdfs:label xml:lang="en">Glass Plate</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/InSituMonitoringAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/InSituMonitoringAct">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfMeasuring"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/is_part_of_process"/>
                        <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfMeasuring"/>
        <cco:definition>An Act Of Measuring that is part of some second process and has as an agent some In-Situ Monitoring System.</cco:definition>
        <rdfs:comment>This class represents the act of continuous observation and measurement within various processes, such as manufacturing, scientific experiments, or environmental monitoring, providing immediate feedback and control.</rdfs:comment>
        <rdfs:label xml:lang="en">In-Situ Monitoring Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/InSituMonitoringSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/InSituMonitoringSystem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/MeasurementArtifactFunction"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/agent_in"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/InSituMonitoringAct"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/System"/>
        <cco:definition>A System that is an agent in some In-Situ Monitoring Act and bears some quality control function.</cco:definition>
        <rdfs:comment>This class includes systems used for continuous observation and measurement within different processes, such as manufacturing, scientific experiments, or environmental monitoring, to provide immediate feedback and control.</rdfs:comment>
        <rdfs:label xml:lang="en">In-Situ Monitoring System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/ManufacturingFacility -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/ManufacturingFacility">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Factory"/>
        <cco:definition xml:lang="en">A factory that is designed for the production of goods</cco:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialArtifactPart -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialArtifactPart">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/PartRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
        <cco:alternative_label xml:lang="en">Part</cco:alternative_label>
        <cco:definition xml:lang="en">A Material Artifact that bears some Part Role</cco:definition>
        <rdfs:label xml:lang="en">Material Artifact Part</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialExtrusionAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialExtrusionAct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing"/>
        <cco:definition>An act of additive manufacturing where material is extruded through a nozzle or orifice and deposited to form successive layers.</cco:definition>
        <rdfs:label xml:lang="en">Material Extrusion Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialExtrusionSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialExtrusionSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem"/>
        <cco:definition>An Additive Manufacturing System designed to perform material extrusion, where material is extruded through a nozzle or orifice to form successive layers.</cco:definition>
        <rdfs:comment>This system is typically used in Fused Deposition Modeling (FDM) and other similar techniques, where thermoplastic material is heated and extruded layer by layer.</rdfs:comment>
        <rdfs:label xml:lang="en">Material Extrusion System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialJettingAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialJettingAct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/ActOfAdditiveManufacturing"/>
        <cco:definition>An act of additive manufacturing where droplets of material are deposited layer by layer to build an object.</cco:definition>
        <rdfs:label xml:lang="en">Material Jetting Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialJettingSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialJettingSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/AMSystem"/>
        <cco:definition>An Additive Manufacturing System designed to perform material jetting, where droplets of material are deposited layer by layer to build an object.</cco:definition>
        <rdfs:comment>This system is often used for creating prototypes and complex parts, where precision and smooth surface finishes are required.</rdfs:comment>
        <rdfs:label xml:lang="en">Material Jetting System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialPreparationAct -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MaterialPreparationAct">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfArtifactProcessing"/>
        <cco:definition>An act of material processing where materials are prepared for subsequent use in additive manufacturing processes, such as mixing, drying, or heating.</cco:definition>
        <rdfs:comment>This class represents the processes involved in preparing raw materials for use in additive manufacturing, including various steps like mixing powders, drying materials to remove moisture, or heating substances to a specific temperature.</rdfs:comment>
        <rdfs:label xml:lang="en">Material Preparation Act</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/MetalPlate -->

    <owl:Class rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/MetalPlate">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/hasRole"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/AdditiveManufacturingOntology/BuildPlatformRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <cco:definition>A metal surface used as a build platform in additive manufacturing processes, supporting the build material and the part being fabricated.</cco:definition>
        <rdfs:comment>This class represents metal plates commonly used as build platforms in various additive manufacturing methods.</rdfs:comment>
        <rdfs:label xml:lang="en">Metal Plate</rdfs:label>
