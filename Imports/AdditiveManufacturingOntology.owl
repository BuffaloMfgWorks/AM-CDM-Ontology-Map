    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/hasDisposition -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/hasDisposition">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <rdfs:label xml:lang="en">has disposition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/AdditiveManufacturingOntology/hasQuality -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/AdditiveManufacturingOntology/hasQuality">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
