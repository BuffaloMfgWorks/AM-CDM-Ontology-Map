                    <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
                    <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ComponentRole"/>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/SYSTEMtoOntology/System"/>
    <rdfs:label xml:lang="en">Additive Manufacturing Subsystem</rdfs:label>
    <cco:definition>A System that bears some Component Role within an Additive Manufacturing System.</cco:definition>
    <rdfs:comment>This class represents subsystems within an Additive Manufacturing System, such as a laser system in a selective laser sintering machine or an extruder in a fused deposition modeling printer, which are essential for the system's operation and success in manufacturing processes.</rdfs:comment>
</owl:Class>

    

<owl:Class rdf:about="http://www.semanticweb.org/SYSTEMtoOntology/InSituMonitoringSystem">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://www.semanticweb.org/SYSTEMtoOntology/System"/>
