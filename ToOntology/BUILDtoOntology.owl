    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/Act -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Act">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:comment>A Process in which at least one Agent plays a causative role.</rdfs:comment>
        <rdfs:label xml:lang="en">Act</rdfs:label>
        <owl:versionInfo rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology"/>
    </owl:Class>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/Agent -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Agent">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/agent_in"/>
                        <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <cco:definition>A Material Entity that is capable of performing Planned Acts</cco:definition>
        <cco:is_curated_in_ontology rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/AgentOntology"/>
        <rdfs:label>Agent</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/Artifact -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Material Entity that was designed by some Agent to realize a certain Function.</rdfs:comment>
        <rdfs:label xml:lang="en">Material Artifact</rdfs:label>
        <owl:versionInfo rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ArtifactOntology"/>
    </owl:Class>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/IntentionalAct -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/IntentionalAct">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Act"/>
        <rdfs:comment>An Act in which at least one Agent plays a causative role and which is prescribed by some Directive Information Content Entity held by at least one of the Agents.</rdfs:comment>
        <rdfs:label xml:lang="en">Planned Act</rdfs:label>
        <rdfs:seeAlso>Intentional Act</rdfs:seeAlso>
        <owl:versionInfo rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology"/>
    </owl:Class>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/Objective -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/Objective">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DirectiveInformationContentEntity"/>
        <cco:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Directive Information Content Entity that prescribes some projected state that some Agent intends to achieve.</cco:definition>
        <cco:definition_source rdf:resource="http://en.wikipedia.org/wiki/Objective_(goal)"/>
        <cco:is_curated_in_ontology rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/AgentOntology"/>
        <rdfs:label xml:lang="en">Objective</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologyrepository.com/CommonCoreOntologies/PlanSpecification -->

    <owl:Class rdf:about="http://www.ontologyrepository.com/CommonCoreOntologies/PlanSpecification">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DirectiveInformationContentEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000178"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Objective"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <cco:definition>A Directive Information Content Entity that prescribes some set of intended Intentional Acts through which some Agent expects to achieve some Objective.</cco:definition>
        <cco:definition_source rdf:resource="https://en.wikipedia.org/w/index.php?title=Plan&amp;oldid=1047320560"/>
        <cco:is_curated_in_ontology rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/AgentOntology"/>
        <rdfs:label>Plan</rdfs:label>
    </owl:Class>
    


